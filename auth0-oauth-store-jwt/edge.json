{
    "version": "1.0",
    "envConfig": {
        "test": {
          "targetServers": [
              {
                "name" : "auth0",
                "host" : "yourdomain.auth0.com",
                "isEnabled" : true,
                "port" : 443,
                "sSLInfo": {
                  "enabled": "true"
                }
              }
          ],
          "caches": [
              {
                "name": "CacheAuth0",
                "description": "Auth0 Cache",
                "expirySettings": {
                    "timeoutInSec": {
                        "value": "300"
                    },
                    "valuesNull": false
                }
              }
          ],
          "kvms": [
              {
                  "name": "auth0Config",
                  "entry": [
                      {
                          "name": "authDomain",
                          "value": "https://yourdomain.auth0.com"
                      },
                      {
                          "name": "apigeeRedirect",
                          "value": "https://org-env.apigee.net/	"
                      }
                  ]
              }
          ]
        },
        "prod": {
          "targetServers": [
              {
                "name" : "auth0",
                "host" : "yourdomain.auth0.com",
                "isEnabled" : true,
                "port" : 443,
                "sSLInfo": {
                  "enabled": "true"
                }
              }
          ],
          "caches": [
              {
                "name": "CacheAuth0",
                "description": "Auth0 Cache",
                "expirySettings": {
                    "timeoutInSec": {
                        "value": "300"
                    },
                    "valuesNull": false
                }
              }
          ],
          "kvms": [
              {
                  "name": "auth0Config",
                  "entry": [
                      {
                          "name": "authDomain",
                          "value": "https://yourdomain.auth0.com"
                      }
                  ]
              }
          ]
        }
    },
    "orgConfig": {
        "apiProducts": [
            {
                "name": "auth0-store-jwt-product",
                "apiResources": [
                    "/",
                    "/**"
                ],
                "approvalType": "auto",
                "attributes": [
                    {
                        "name": "description",
                        "value": "Auth0 store jwt product"
                    }
                ],
                "description": "Auth0 Store JWT Product",
                "displayName": "Auth0 OAuth Store JWT Product",
                "environments": [
                    "test",
                    "prod"
                ],
                "proxies": [
                    "auth0_oauth_store_jwt",
                    "auth0_test_proxy"
                ],
                "quota": "10000",
                "quotaInterval": "1",
                "quotaTimeUnit": "month",
                "scopes": []
            }
        ],
        "developers": [
            {
                "email": "john@example.com",
                "firstName": "John",
                "lastName": "Example",
                "userName": "johnexample",
                "attributes": []
            }
        ],
        "developerApps": {
            "john@example.com": [
                {
                    "name": "auth0-store-jwt-app",
                    "apiProducts": [ "auth0-store-jwt-product" ],
                    "callbackUrl": "https://callback.io",
                    "scopes": []
                }
            ]
        }
    }
}
